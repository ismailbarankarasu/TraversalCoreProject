@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="tr">
@await Component.InvokeAsync("_MainLayoutHeadComponentPartial")
<body class="g-sidenav-show  bg-gray-100">
    @await Component.InvokeAsync("_MainLayoutSidebarComponentPartial")
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        @await Component.InvokeAsync("_MainLayoutNavbarComponentPartial")
        <div class="container-fluid py-4">
            @RenderBody()
            @await Component.InvokeAsync("_MemberLayoutFooterComponentPartial")
        </div>
    </main>
    @await Component.InvokeAsync("_MemberLayoutScriptComponentPartial")
</body>

</html>